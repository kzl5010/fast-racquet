{
  "name": "fastracquet",
  "version": "1.0.0",
  "description": "== README",
  "babel": {
    "presets": [
      "flow"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kzl5010/fast-racquet.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kzl5010/fast-racquet/issues"
  },
  "homepage": "https://github.com/kzl5010/fast-racquet#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browserify": "^14.3.0",
    "moment": "^2.18.1",
    "parse-address": "0.0.6",
    "react": "^15.5.3",
    "react-bootstrap": "^0.31.0",
    "react-datepicker": "^0.46.0",
    "react-dom": "^15.5.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-places-autocomplete": "^5.3.1",
    "react-redux": "^5.0.3",
    "react-router": "^2.0.0",
    "react-script-loader": "0.0.1",
    "react-slick": "^0.14.11",
    "react-stripe-checkout": "^2.4.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-lint": "^1.10.2",
    "stripe": "^4.22.0",
    "webpack": "^2.3.3"
  },
  "engines": {
    "node": "7.8.0",
    "npm": "4.5.0"
  },
  "scripts": {
    "postinstall": "webpack",
    "flow": "flow",
    "test": "jest"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "semi": 2
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "flow-bin": "^0.57.3",
    "jest": "^21.2.1",
    "react-test-renderer": "^15.5.4",
    "remote-redux-devtools": "^0.5.12"
  }
}
